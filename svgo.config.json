{
  "multipass": true,
  "plugins": [
    {
      "cleanupIDs": false
    },
    {
      "cleanupNumericValues": {
        "floatPrecision": 2
      }
    },
    {
      "mergePaths": false
    },
    {
      "removeOffCanvasPaths": true
    },
    {
      "removeTitle": true
    },
    {
      "sortAttrs": true
    }
  ],
  "js2svg": {
    "pretty": true,
    "indent": 2
  }
}
